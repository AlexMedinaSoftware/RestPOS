<Page x:Class="Dash_Config"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:RestPOS"
      mc:Ignorable="d" 
      d:DesignHeight="733" d:DesignWidth="1166"
      Background="{DynamicResource LightColor}"
      Title="Mesas">
    <Page.Resources>
        <ResourceDictionary x:Name="Recursos" Source="/Assets/Icons.xaml"/>
    </Page.Resources>
    <Grid>
        <ScrollViewer VerticalAlignment="Stretch" HorizontalAlignment="Stretch" Margin="20">
            <UniformGrid x:Name="Configs_Grid" Background="{DynamicResource LightColor}" Columns="1" VerticalAlignment="Top" HorizontalAlignment="Left"/>
        </ScrollViewer>
    </Grid>
</Page>
